@model IEnumerable<UserProject.Models.User> //זה המודל שלך 

<h1>Users</h1>
<form asp-action="Ella" method="get">
    <input type="text" name="name" placeholder="Search by name" />
    <button type="submit">Search</button>
</form>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>ID Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.IDNumber</td>
            </tr>
        }
       
    </tbody>
</table>
